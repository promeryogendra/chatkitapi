{
  "_meta": {
    "sources": [
      "loopback/common/models",
      "loopback/server/models",
      "../common/models",
      "./models"
    ],
    "mixins": [
      "loopback/common/mixins",
      "loopback/server/mixins",
      "../common/mixins",
      "./mixins"
    ]
  },
  "User": {
    "dataSource": "mongodb",
    "public": false,
    "options": {
      "emailVerificationRequired": true
    }
  },
  "Email": {
    "dataSource": "emailDs"
  },
  "AccessToken": {
    "dataSource": "mongodb",
    "public": false
  },
  "ACL": {
    "dataSource": "mongodb",
    "public": false
  },
  "RoleMapping": {
    "dataSource": "mongodb",
    "public": false,
    "options": {
      "strictObjectIDCoercion": true
    }
  },
  "Role": {
    "dataSource": "mongodb",
    "public": false
  },
  "customer": {
    "dataSource": "mongodb",
    "public": true,
    "options": {
      "emailVerificationRequired": true
    }
  },
  "friends": {
    "dataSource": "mongodb",
    "public": true
  },
  "requests": {
    "dataSource": "mongodb",
    "public": true
  },
  "messages": {
    "dataSource": "mongodb",
    "public": true
  }
}
